{
    "logs": {
        "logs_collected": {
            "files": {
                "collect_list": [
                    {
                        "file_path": "/var/lib/nomad/alloc/*/alloc/logs/snowl-mysql-mysql.stderr.0",
                        "log_group_name": "$[clustername]__snowl-mysql__job",
                        "log_stream_name": "$[hostname]__stderr",
                        "timestamp_format": "%Y-%m-%dT%H:%M:%S"
                    },
                    {
                        "file_path": "/var/lib/nomad/alloc/*/alloc/logs/snowl-wds-origin-nginx.stdout.0",
                        "log_group_name": "$[clustername]__snowl-wds-origin__job",
                        "log_stream_name": "$[hostname]__stdout",
                        "timestamp_format": "%Y/%m/%d %H:%M:%S"
                    },
                    {
                        "file_path": "/var/lib/nomad/alloc/*/alloc/logs/snowl-wds-origin-nginx.stderr.0",
                        "log_group_name": "$[clustername]__snowl-wds-origin__job",
                        "log_stream_name": "$[hostname]__stderr"
                    },
                    {
                        "file_path": "/mnt/host/snowl-wds-origin/*/log/access.log",
                        "log_group_name": "$[clustername]__snowl-wds-origin__nginx",
                        "log_stream_name": "$[hostname]__access",
                        "timestamp_format": "[%d/%m/%Y:%H:%M:%S %z]"
                    },
                    {
                        "file_path": "/mnt/host/snowl-wds-origin/*/log/error.log",
                        "log_group_name": "$[clustername]__snowl-wds-origin__nginx",
                        "log_stream_name": "$[hostname]__error",
                        "timestamp_format": "%Y/%m/%d %H:%M:%S",
                        "multi_line_start_pattern": "{timestamp_format}"
                    },
                    {
                        "file_path": "/var/lib/nomad/alloc/*/alloc/logs/snowl-wms-origin-nginx.stdout.0",
                        "log_group_name": "$[clustername]__snowl-wms-origin__job",
                        "log_stream_name": "$[hostname]__stdout",
                        "timestamp_format": "%Y/%m/%d %H:%M:%S"
                    },
                    {
                        "file_path": "/var/lib/nomad/alloc/*/alloc/logs/snowl-wms-origin-nginx.stderr.0",
                        "log_group_name": "$[clustername]__snowl-wms-origin__job",
                        "log_stream_name": "$[hostname]__stderr"
                    },
                    {
                        "file_path": "/mnt/host/snowl-wms-origin/*/log/access.log",
                        "log_group_name": "$[clustername]__snowl-wms-origin__nginx",
                        "log_stream_name": "$[hostname]__access",
                        "timestamp_format": "[%d/%m/%Y:%H:%M:%S %z]"
                    },
                    {
