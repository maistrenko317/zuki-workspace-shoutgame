                        "log_stream_name": "$[hostname]__stdout",
                        "timestamp_format": "%Y/%m/%d %H:%M:%S"
                    },
                    {
                        "file_path": "/var/lib/nomad/alloc/*/alloc/logs/snowl-web-origin-nginx.stderr.0",
                        "log_group_name": "$[clustername]__snowl-web-origin__job",
                        "log_stream_name": "$[hostname]__stderr"
