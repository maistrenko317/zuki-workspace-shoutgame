                        "log_group_name": "$[clustername]__snowl-web-origin__nginx",
                        "log_stream_name": "$[hostname]__error",
                        "timestamp_format": "%Y/%m/%d %H:%M:%S",
                        "multi_line_start_pattern": "{timestamp_format}"
                    },
                    {
                        "file_path": "/var/lib/nomad/alloc/*/alloc/logs/snowl-socketio.stdout.0",
                        "log_group_name": "$[clustername]__snowl-socketio__job",
                        "log_stream_name": "$[hostname]__stdout"
                    },
                    {
                        "file_path": "/var/lib/nomad/alloc/*/alloc/logs/snowl-socketio.stderr.0",
