{
    "name": "tipping-point",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --aot --host 0.0.0.0 --disable-host-check --port 4300 --ssl",
        "build": "node pre-build.js && ng build --prod --stats-json --base-href=/play/ --build-optimizer --sourcemap && node post-build.js",
        "buildStage": "node pre-build.js && ng build --prod --env=stage --stats-json --base-href=/play/ --build-optimizer --sourcemap && node post-build.js",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "bundle-report": "webpack-bundle-analyzer dist/stats.json",
        "postinstall": "npm rebuild node-sass"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^5.0.0",
        "@angular/common": "^5.0.0",
        "@angular/compiler": "^5.0.0",
        "@angular/core": "^5.0.0",
        "@angular/forms": "^5.0.0",
        "@angular/http": "^5.0.0",
        "@angular/platform-browser": "^5.0.0",
        "@angular/platform-browser-dynamic": "^5.0.0",
        "@angular/platform-server": "^5.0.0",
        "@angular/router": "^5.0.0",
        "angular-tslint-rules": "^1.0.3",
        "angular2-text-mask": "^8.0.3",
        "card-validator": "^4.1.1",
        "cerialize": "^0.1.15",
        "core-js": "^2.4.1",
        "crypto-js": "^3.1.9-1",
        "date-fns": "^2.0.0-alpha.7",
        "intl": "^1.2.5",
        "offline-js": "^0.7.19",
        "progressbar.js": "^1.0.1",
        "rxjs": "^5.5.2",
        "scrolling-element": "^1.0.2",
        "scrypt-async": "^1.3.1",
        "socket.io-client": "^2.0.3",
        "text-mask-addons": "^3.6.0",
        "uuid": "^3.1.0",
        "vx-components": "^1.0.1-beta.8",
        "zone.js": "^0.8.14"
    },
    "devDependencies": {
        "@angular/cli": "^1.5.0",
        "@angular/compiler-cli": "^5.0.0",
        "@angular/language-service": "^4.0.0",
        "@types/card-validator": "^4.1.0",
        "@types/crypto-js": "^3.1.36",
        "@types/jasmine": "~2.5.53",
        "@types/jasminewd2": "~2.0.2",
        "@types/scrypt-async": "^1.2.27",
        "@types/socket.io-client": "^1.4.30",
        "codelyzer": "~3.0.1",
        "jasmine-core": "~2.6.2",
        "jasmine-spec-reporter": "~4.1.0",
        "karma": "~1.7.0",
        "karma-chrome-launcher": "~2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.1.2",
        "ts-node": "~3.0.4",
        "tslint": "~5.3.2",
        "typescript": "2.4.2",
        "webpack-bundle-analyzer": "^2.9.0"
    }
